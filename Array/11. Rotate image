class Solution:
    def rotate(self, matrix: List[List[int]]) -> None:
        """
        Do not return anything, modify matrix in-place instead.
        """
        mat = []
        n = len(matrix)
      
        # transpose matrix 
        for c in range(n):
            row = []
            for r in range(n):
                # iterate reverse row
                row.append(matrix[-r-1][c])
            mat.append(row)
             
        matrix[:] = mat[:]
